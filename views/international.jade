extends ./layout.jade

block title
  title 复杂网络与安全研究实验室 | 国际合作

block content
  table.ui.celled.table
    thead
      tr
        th 大学/企业
        th 所属国家
        th 合作项目
    tbody
      for collaboration in collaborations
        tr
          td #{collaboration.agency_name}
          td #{collaboration.agency_country}
          td #{collaboration.project_name}
